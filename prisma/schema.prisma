generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Sobe {
  id        BigInt   @id @default(autoincrement())
  brojSobe  String   @unique

}
model notes {
  id    BigInt @id @default(autoincrement())
  title String
}
model users {
  id        BigInt   @id @default(autoincrement())
  name      String
  password  String
  email     String   @unique
  role      String
}
